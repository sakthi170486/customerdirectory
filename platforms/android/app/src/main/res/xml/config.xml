<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.navigator" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Accelerometer">
        <param name="android-package" value="org.apache.cordova.devicemotion.AccelListener" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="Battery">
        <param name="android-package" value="org.apache.cordova.batterystatus.BatteryListener" />
    </feature>
    <name>Customer Directory</name>
    <description>
        A simple customer directory application
    </description>
    <author email="sakthipit@gmail.com" href="#">
        Sakthivel
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon qualifier="ldpi" src="www/res/icons/android/drawable-ldpi-icon.png" />
    <icon qualifier="mdpi" src="www/res/icons/android/drawable-mdpi-icon.png" />
    <icon qualifier="hdpi" src="www/res/icons/android/drawable-hdpi-icon.png" />
    <icon qualifier="xhdpi" src="www/res/icons/android/drawable-xhdpi-icon.png" />
    <icon qualifier="xxhdpi" src="www/res/icons/android/drawable-xxhdpi-icon.png" />
    <icon qualifier="xxxhdpi" src="www/res/icons/android/drawable-xxxhdpi-icon.png" />
    <splash qualifier="land-ldpi" src="www/res/screens/android/drawable-land-ldpi-screen.png" />
    <splash qualifier="land-mdpi" src="www/res/screens/android/drawable-land-mdpi-screen.png" />
    <splash qualifier="land-hdpi" src="www/res/screens/android/drawable-land-hdpi-screen.png" />
    <splash qualifier="land-xhdpi" src="www/res/screens/android/drawable-land-xhdpi-screen.png" />
    <splash qualifier="land-xxhdpi" src="www/res/screens/android/drawable-land-xxhdpi-screen.png" />
    <splash qualifier="land-xxxhdpi" src="www/res/screens/android/drawable-land-xxxhdpi-screen.png" />
    <splash qualifier="port-ldpi" src="www/res/screens/android/drawable-port-ldpi-screen.png" />
    <splash qualifier="port-mdpi" src="www/res/screens/android/drawable-port-mdpi-screen.png" />
    <splash qualifier="port-hdpi" src="www/res/screens/android/drawable-port-hdpi-screen.png" />
    <splash qualifier="port-xhdpi" src="www/res/screens/android/drawable-port-xhdpi-screen.png" />
    <splash qualifier="port-xxhdpi" src="www/res/screens/android/drawable-port-xxhdpi-screen.png" />
    <splash qualifier="port-xxxhdpi" src="www/res/screens/android/drawable-port-xxxhdpi-screen.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="SplashScreen" value="screen" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="iosPersistentFileLocation" value="Compatibility" />
    <preference name="iosExtraFilesystems" value="library,library-nosync,documents,documents-nosync,cache,bundle,root" />
    <preference name="AndroidExtraFilesystems" value="files,files-external,documents,sdcard,cache,cache-external,assets,root" />
</widget>
